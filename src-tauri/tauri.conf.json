{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": ["../dist", "../src/themes", "../src/tabs"],
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "task-manager",
  "version": "0.0.0",
  "identifier": "dev.pgil.forkbomb.taskmgr",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "task-manager",
        "width": 1080,
        "height": 700,
        "minWidth": 1080,
        "minHeight": 700
      }
    ],
    "security": {
      "csp": null
    }
  }
}